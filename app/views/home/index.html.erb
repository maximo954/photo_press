<div class="jumbotron center">

  <h2>Welcome To My PhotoPress App</h2>
  <p>Find me in app/views/home/index.html.erb</p>

  <% if user_signed_in? %>
    <%= link_to "Edit Profile", edit_user_registration_path, class: "btn btn-default" %>
    <%= link_to "Logout", destroy_user_session_path, method: :delete, class: "btn btn-primary"%>
  <% else %>
    <%= link_to "Login", user_session_path, class: "btn btn-default"%>
    <%= link_to "Join", new_user_registration_path, class: "btn btn-primary"%>
  <% end %>



</div>
